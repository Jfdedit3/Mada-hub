-- Loading the notification library
local NotificationLoad = loadstring(game:HttpGet('https://raw.githubusercontent.com/treeofplant/Notif/main/library.lua', true))()

if not NotificationLoad then
    error("The notification library failed to load.")
end

local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local TitleLabel = Instance.new("TextLabel")
local CloseButton = Instance.new("TextButton")
local ButtonContainer = Instance.new("Frame")
local FooterLabel = Instance.new("TextLabel")
local ToggleVisibilityButton = Instance.new("TextButton")

-- Notification function
local function createNotification(title, description, timeout)
    NotificationLoad:NewNotification({
        ["Mode"] = "Custom",
        ["Title"] = title,
        ["Description"] = description,
        ["Notification_Image"] = "rbxassetid://",
        ["Notification_Sound"] = "rbxassetid://5020929359",
        ["Timeout"] = timeout or 5,
        ["Audio"] = false 
    })
end

-- Function to apply rounded corners
local function applyCorners(instance, cornerRadius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, cornerRadius)
    corner.Parent = instance
end

-- Creating the UI
ScreenGui.Name = "MyScreenGui"
ScreenGui.Parent = game.CoreGui

MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
MainFrame.Position = UDim2.new(0.5, -200, 0.5, -150)
MainFrame.Size = UDim2.new(0, 400, 0, 300)
MainFrame.BackgroundTransparency = 0.1
applyCorners(MainFrame, 12)

TitleLabel.Name = "TitleLabel"
TitleLabel.Parent = MainFrame
TitleLabel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
TitleLabel.Size = UDim2.new(1, 0, 0, 30)
TitleLabel.Font = Enum.Font.GothamBold
TitleLabel.Text = "Mada hub v3.3"
TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TitleLabel.TextSize = 16
applyCorners(TitleLabel, 10)

CloseButton.Name = "CloseButton"
CloseButton.Parent = MainFrame
CloseButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
CloseButton.Position = UDim2.new(0.95, -20, 0, 5)
CloseButton.Size = UDim2.new(0, 20, 0, 20)
CloseButton.Font = Enum.Font.GothamBold
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.TextSize = 14
applyCorners(CloseButton, 5)

CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

ButtonContainer.Name = "ButtonContainer"
ButtonContainer.Parent = MainFrame
ButtonContainer.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
ButtonContainer.Position = UDim2.new(0, 0, 0, 30)
ButtonContainer.Size = UDim2.new(1, 0, 1, -70)
applyCorners(ButtonContainer, 10)

-- Button array
local buttons = {
    {text = "KOLINSKI admin", url = "https://raw.githubusercontent.com/Jfdedit3/KOLINSKI/refs/heads/main/KOLINSKI.lua"},
    {text = "Cmd Fe", url = "https://raw.githubusercontent.com/lxte/cmd/main/testing-main.lua"},
    {text = "Honoka Executor", url = "https://raw.githubusercontent.com/Jfdedit3/honoka-executor/main/Honoka%20executor%20byz4trox"},
    {text = "Velocity Ware", url = "https://raw.githubusercontent.com/ImNotRox1/VWare/main/Universal.lua"},
    {text = "Nameless Music Brook", url = "https://raw.githubusercontent.com/Jfdedit3/Nameless-hub-/refs/heads/main/Nameless%20musique"},
    {text = "Aimbot", url = "https://raw.githubusercontent.com/Jfdedit3/Aimbot-aim-assist-lock-/refs/heads/main/Aimbot-aim-assist-lock"},
    {text = "Fps Script", url = "https://raw.githubusercontent.com/Jfdedit3/Fps/refs/heads/main/fps.lua"} -- Nouveau bouton ajout√© ici
}

-- Function to create a stylized button
local function createButton(text, url, position)
    local button = Instance.new("TextButton")
    button.Name = text
    button.Parent = ButtonContainer
    button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    button.Position = UDim2.new(0, 10, 0, position)
    button.Size = UDim2.new(1, -20, 0, 20)
    button.Font = Enum.Font.Gotham
    button.Text = text
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.TextSize = 14
    applyCorners(button, 8)

    button.MouseButton1Click:Connect(function()
        createNotification(text, text .. " is running.", 3)

        if not url:match("^https://raw.githubusercontent.com/") then
            createNotification("Error", "Invalid script URL.", 5)
            return
        end

        local success, err = pcall(function()
            local startTime = tick()
            local scriptCode = game:HttpGet(url, true)
            if not scriptCode then
                error("Failed to download the script.")
            end
            loadstring(scriptCode)()
            local endTime = tick()
            local executionTime = endTime - startTime

            createNotification("Success", text .. " executed in " .. string.format("%.2f", executionTime) .. " seconds.", 5)
        end)

        if not success then
            createNotification("Error", "Failed to execute: " .. err, 5)
        end
    end)

    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    end)
    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    end)
end

-- Create buttons dynamically
for index, buttonInfo in ipairs(buttons) do
    createButton(buttonInfo.text, buttonInfo.url, (index - 1) * 25)
end

-- Add textbox and execute button for custom scripts
local ScriptTextBox = Instance.new("TextBox")
local ExecuteButton = Instance.new("TextButton")

-- TextBox settings
ScriptTextBox.Name = "ScriptTextBox"
ScriptTextBox.Parent = ButtonContainer
ScriptTextBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
ScriptTextBox.Position = UDim2.new(0, 10, 0, #buttons * 25)
ScriptTextBox.Size = UDim2.new(0.7, -20, 0, 20)
ScriptTextBox.Font = Enum.Font.Gotham
ScriptTextBox.PlaceholderText = "Enter script"
ScriptTextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
ScriptTextBox.TextSize = 14
applyCorners(ScriptTextBox, 8)

-- ExecuteButton settings
ExecuteButton.Name = "ExecuteButton"
ExecuteButton.Parent = ButtonContainer
ExecuteButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
ExecuteButton.Position = UDim2.new(0.7, 10, 0, #buttons * 25)
ExecuteButton.Size = UDim2.new(0.25, -20, 0, 20)
ExecuteButton.Font = Enum.Font.Gotham
ExecuteButton.Text = "Run"
ExecuteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ExecuteButton.TextSize = 14
applyCorners(ExecuteButton, 8)

-- Execute script from TextBox
ExecuteButton.MouseButton1Click:Connect(function()
    local scriptText = ScriptTextBox.Text
    if scriptText and scriptText ~= "" then
        createNotification("Script", "Running your script...", 3)
        local success, err = pcall(function()
            loadstring(scriptText)()
        end)
        if not success then
            createNotification("Error", "Failed to run script: " .. err, 5)
        else
            createNotification("Success", "Script executed successfully!", 5)
        end
    else
        createNotification("Error", "No script entered.", 3)
    end
end)

-- Add footer text
FooterLabel.Name = "FooterLabel"
FooterLabel.Parent = MainFrame
FooterLabel.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
FooterLabel.Position = UDim2.new(0, 10, 1, -30)
FooterLabel.Size = UDim2.new(1, -20, 0, 30)
FooterLabel.Font = Enum.Font.GothamBold
FooterLabel.Text = "z4trox | v3.3 | https://discord.gg/c24QBGCu3V"
FooterLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
FooterLabel.TextSize = 12
FooterLabel.BackgroundTransparency = 1

-- Function to toggle UI visibility
local function toggleUIVisibility()
    if MainFrame.Visible then
        -- Move the GUI up until it's out of view
        MainFrame:TweenPosition(UDim2.new(0.5, -200, -1, 0), Enum.EasingDirection.InOut, Enum.EasingStyle.Quad, 0.75, true, 
        function()
            MainFrame.Visible = false  -- Hide the GUI after the animation
        end)
    else
        -- Show the GUI and move it back into view
        MainFrame.Visible = true
        MainFrame.Position = UDim2.new(0.5, -200, -1, 0)  -- Set starting position above the view
        MainFrame:TweenPosition(UDim2.new(0.5, -200, 0.5, -150), Enum.EasingDirection.InOut, Enum.EasingStyle.Quad, 0.75, true)
    end
end

-- ToggleVisibilityButton settings
ToggleVisibilityButton.Name = "ToggleVisibilityButton"
ToggleVisibilityButton.Parent = ScreenGui
ToggleVisibilityButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)  -- Couleur noire
ToggleVisibilityButton.Position = UDim2.new(0, 10, 0, 5)
ToggleVisibilityButton.Size = UDim2.new(0, 80, 0, 30)
ToggleVisibilityButton.Font = Enum.Font.GothamBold
ToggleVisibilityButton.Text = "Toggle UI"
ToggleVisibilityButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleVisibilityButton.TextSize = 14
applyCorners(ToggleVisibilityButton, 8)

-- Connect the toggle button to the function
ToggleVisibilityButton.MouseButton1Click:Connect(toggleUIVisibility)

-- Animation for introduction
local function showIntroductionAnimation()
    MainFrame.Position = UDim2.new(0.5, -200, 0.5, -150)
    MainFrame.Size = UDim2.new(0, 0, 0, 0)
    MainFrame.Visible = true
    MainFrame:TweenSize(UDim2.new(0, 400, 0, 300), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.75, true)
    MainFrame:TweenPosition(UDim2.new(0.5, -200, 0.5, -150), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.75, true)
end

-- Call the introduction animation when the script starts
showIntroductionAnimation()
